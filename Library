				Health.Text = "100"
				Health.TextColor3 = Color3.fromRGB(231, 231, 231)
				Health.TextSize = 15.000
				Health.TextWrapped = true

				UICorner.CornerRadius = UDim.new(0, 6)
				UICorner.Parent = TargetHUD

				Line.Name = "Line"
