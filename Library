				UIGradient_2.Parent = Glow

				Name.Name = "Name"
				Name.Parent = TargetHUD
				Name.BackgroundColor3 = Color3.fromRGB(162, 162, 162)
				Name.BackgroundTransparency = 1.000
				Name.Position = UDim2.new(0.320253134, 0, 0.101265907, 0)
