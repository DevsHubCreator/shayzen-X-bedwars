				UICorner_2.Parent = Line

				LineBG.Name = "LineBG"
				LineBG.Parent = TargetHUD
				LineBG.BackgroundColor3 = Color3.fromRGB(27, 27, 27)
				LineBG.BorderSizePixel = 0
				LineBG.Position = UDim2.new(0.337569118, 0, 0.677131355, 0)
				LineBG.Size = UDim2.new(0, 144, 0, 12)

				UICorner_3.Parent = LineBG

				Glow_2.Name = "Glow"
				Glow_2.Parent = TargetHUD
				Glow_2.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
				Glow_2.BackgroundTransparency = 1.000
				Glow_2.Position = UDim2.new(0.264069319, 0, 0.490773618, 0)
				Glow_2.Size = UDim2.new(0, 178, 0, 40)
				Glow_2.Image = "rbxassetid://12822896774"
				Glow_2.ImageTransparency = 0.560

				UIGradient_4.Color = ColorSequence.new{ColorSequenceKeypoint.new(0.00, Color3.fromRGB(239, 231, 248)), ColorSequenceKeypoint.new(0.35, Color3.fromRGB(184, 145, 251)), ColorSequenceKeypoint.new(0.63, Color3.fromRGB(144, 86, 253)), ColorSequenceKeypoint.new(1.00, Color3.fromRGB(104, 24, 255))}
				UIGradient_4.Parent = Glow_2

				Head.Name = "Head"
				Head.Parent = TargetHUD
				Head.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
				Head.BackgroundTransparency = 1.000
				Head.Position = UDim2.new(0.0381752327, 0, 0.101265669, 0)
				Head.Size = UDim2.new(0, 60, 0, 62)
				Head.Image = "rbxassetid://6228441155"

				UICorner_4.CornerRadius = UDim.new(0, 10)
				UICorner_4.Parent = Head

				UIGradient_5.Color = ColorSequence.new{ColorSequenceKeypoint.new(0.00, Color3.fromRGB(193, 183, 232)), ColorSequenceKeypoint.new(1.00, Color3.fromRGB(116, 116, 232))}
				UIGradient_5.Parent = TargetHUD
				
				spawn(function()
					while task.wait() do
						if UIGradient.Rotation < 360 then
							UIGradient.Rotation = UIGradient.Rotation + 0.65
						else
							UIGradient.Rotation = 2
						end
					end
				end)
				spawn(function()
					while task.wait() do
						if UIGradient_2.Rotation < 360 then
							UIGradient_2.Rotation = UIGradient_2.Rotation + 0.65
						else
							UIGradient_2.Rotation = 2
						end
					end
				end)
				spawn(function()
					while task.wait() do
						if UIGradient_4.Rotation < 360 then
							UIGradient_4.Rotation = UIGradient_4.Rotation + 0.65
						else
							UIGradient_4.Rotation = 2
						end
					end
				end)
				spawn(function()
					while task.wait() do
						if UIGradient_5.Rotation < 360 then
							UIGradient_5.Rotation = UIGradient_5.Rotation + 0.65
						else
							UIGradient_5.Rotation = 2
						end
					end
				end)
				spawn(function()
					while task.wait() do
						if UIGradient_6.Rotation < 360 then
							UIGradient_6.Rotation = UIGradient_6.Rotation + 0.65
						else
							UIGradient_6.Rotation = 2
						end
					end
				end)
				
				local options = {}
				
				function options:ChangeProprs(name, sync, health, maxHealth)
					name = name or "...";
					health = math.round(health) or 100
					maxHealth = math.round(maxHealth) or 100
					
					TargetHUD.Visible = sync;
					
					if sync then
						Name.Text = tostring(name)
						Health.Text = tostring(health)

						if (health < 1) then
							ts:Create(Line,TweenInfo.new(0.15, Enum.EasingStyle.Quad),{Size = UDim2.new(0, 0, 0, 12)}):Play()
							ts:Create(Line,TweenInfo.new(0.25, Enum.EasingStyle.Quad),{Transparency = 1}):Play()
						elseif (health > 1) then
							ts:Create(Line,TweenInfo.new(0.15, Enum.EasingStyle.Quad),{Transparency = 0}):Play()
							ts:Create(Line,TweenInfo.new(0.15, Enum.EasingStyle.Quad),{Size = UDim2.new(0, health / maxHealth, 0, 12)}):Play()
						end
					end
				end
				
				return options;
			end
		end
		
		return objects;
	end
	
	function main_objects:CreateWatermark(name,build)
		local Watermark = Instance.new("Frame")
		local UICorner = Instance.new("UICorner")
		local UIStroke = Instance.new("UIStroke")
		local UIGradient = Instance.new("UIGradient")
		local Glow = Instance.new("ImageLabel")
		local UIGradient_2 = Instance.new("UIGradient")
		local text = Instance.new("TextLabel")
		local Shadow = Instance.new("ImageLabel")
		local UICorner_2 = Instance.new("UICorner")
		local UIGradient_3 = Instance.new("UIGradient")
		
		Watermark.Name = "Watermark"
		Watermark.Parent = shayzenSxz
		Watermark.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		Watermark.Position = UDim2.new(0.0140000004, 0, 0.0219999999, 0)
		Watermark.Size = UDim2.new(0, 195, 0, 34)

		UICorner.CornerRadius = UDim.new(0, 6)
		UICorner.Parent = Watermark

		UIStroke.Color = Color3.fromRGB(200, 200, 200)
		UIStroke.Thickness = 2.700
		UIStroke.Transparency = 0.250
		UIStroke.Parent = Watermark

		UIGradient.Color = ColorSequence.new{ColorSequenceKeypoint.new(0.00, Color3.fromRGB(193, 183, 232)), ColorSequenceKeypoint.new(1.00, Color3.fromRGB(116, 116, 232))}
		UIGradient.Parent = UIStroke

		Glow.Name = "Glow"
		Glow.Parent = Watermark
		Glow.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		Glow.BackgroundTransparency = 1.000
		Glow.Position = UDim2.new(-0.108533382, 0, -0.470588237, 0)
		Glow.Size = UDim2.new(0, 236, 0, 66)
		Glow.ZIndex = 0
		Glow.Image = "rbxassetid://12822896774"
		Glow.ImageTransparency = 0.110

		UIGradient_2.Color = ColorSequence.new{ColorSequenceKeypoint.new(0.00, Color3.fromRGB(149, 157, 213)), ColorSequenceKeypoint.new(0.35, Color3.fromRGB(119, 134, 195)), ColorSequenceKeypoint.new(0.63, Color3.fromRGB(123, 138, 203)), ColorSequenceKeypoint.new(1.00, Color3.fromRGB(116, 131, 247))}
		UIGradient_2.Parent = Glow

		text.Name = "text"
		text.Parent = Watermark
		text.BackgroundColor3 = Color3.fromRGB(162, 162, 162)
		text.BackgroundTransparency = 1.000
		text.Size = UDim2.new(0, 195, 0, 34)
		text.ZIndex = 2
		text.Font = Enum.Font.GothamBold
		text.Text =  tostring(name.." | alpha | "..build);
		text.TextColor3 = Color3.fromRGB(231, 231, 231)
		text.TextSize = 15.000
		text.TextWrapped = true

		Shadow.Name = "Shadow"
		Shadow.Parent = Watermark
		Shadow.Active = true
		Shadow.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		Shadow.BackgroundTransparency = 1.000
		Shadow.Size = UDim2.new(0, 194, 0, 34)
		Shadow.Image = "rbxassetid://4576475446"
		Shadow.ImageTransparency = 0.850

		UICorner_2.CornerRadius = UDim.new(0, 6)
		UICorner_2.Parent = Shadow

		UIGradient_3.Color = ColorSequence.new{ColorSequenceKeypoint.new(0.00, Color3.fromRGB(193, 183, 232)), ColorSequenceKeypoint.new(1.00, Color3.fromRGB(116, 116, 232))}
		UIGradient_3.Parent = Watermark
		
		spawn(function()
			while task.wait() do
				if UIGradient.Rotation < 360 then
					UIGradient.Rotation = UIGradient.Rotation +  0.60
				else
					UIGradient.Rotation = 2
				end
			end
		end)
		spawn(function()
			while task.wait() do
				if UIGradient_2.Rotation < 360 then
					UIGradient_2.Rotation = UIGradient_2.Rotation +  0.60
				else
					UIGradient_2.Rotation = 2
				end
			end
		end)
		spawn(function()
			while task.wait() do
				if UIGradient_3.Rotation < 360 then
					UIGradient_3.Rotation = UIGradient_3.Rotation +  0.60
				else
					UIGradient_3.Rotation = 2
				end
			end
		end)
		
		Watermark.Active = true;
		Watermark.Draggable = true;
		
		Watermark.Position = UDim2.new(0.0140000004, 0,0.127, 0)
		Watermark["text"].TextTransparency = 1

		wait(0.5)
		ts:Create(text, TweenInfo.new(0.10, Enum.EasingStyle.Quad), {TextTransparency = 0}):Play()
		ts:Create(Watermark, TweenInfo.new(1.25, Enum.EasingStyle.Quad), {Position = UDim2.new(0.0140000004, 0, 0.0219999999, 0)}):Play()
		
	end
	
	return main_objects;
end

